{
    "geode": "4.10.0",
    "gd": {
        "android": "2.2074",
        "win": "2.2074"
    },
    "version": "v1.0.0",
    "id": "noyan.integratednoyansdemonslist",
    "name": "Integrated Noyans Demons List",
    "developer": "hiimjasmine00",
    "description": "Simple Noyans Demons List integration.",
    "resources": {
        "files": [
            "resources/*.frag",
            "resources/*.vert"
        ],
        "sprites": [
            "resources/*.png"
        ]
    },
    "settings": {
        "enable-rank": {
            "name": "Enable Demon Ranking Text",
            "description": "Whether or not to display the demon ranking text.",
            "type": "bool",
            "default": true
        },
        "white-rank": {
            "name": "White Demon Ranking Text",
            "description": "Whether or not to display the demon ranking text in white.",
            "type": "bool",
            "enable-if": "enable-rank",
            "default": false
        }
    },
    "dependencies": {
        "geode.node-ids": ">=v1.9.0"
    },
    "links": {
        "community": "https://discord.gg/QVKmbvBXA7",
        "source": "https://github.com/hiimjasmine00/IntegratedDemonlist",
        "homepage": "https://www.hiimjasmine00.com"
    },
    "tags": [
